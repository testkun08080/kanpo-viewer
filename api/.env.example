# Kanpo PDF API 環境設定ファイル
# 本番環境用の設定例

# 基本設定
PROJECT_NAME=Kanpo PDF API
PROJECT_DESCRIPTION=PDF download API for Kanpo documents
VERSION=0.1.0
ROOT_PATH=/api

# セキュリティ設定（本番環境 - 必ず変更してください）
KANPO_API_KEY=dev-api-key-12345
ENVIRONMENT=production
DEBUG=false

# CORS設定（カンマ区切りで複数ドメイン指定可能）
ALLOWED_ORIGINS=https://kanpo-viewer.com

# ログレベル
LOG_LEVEL=INFO

# サービス制限
MAX_FILE_SIZE=10485760  # 10MB
TIMEOUT=30