# Kanpo PDF API 開発環境設定

# 基本設定
PROJECT_NAME=Kanpo PDF API
PROJECT_DESCRIPTION=PDF download API for Kanpo documents
VERSION=0.0.0
ROOT_PATH=/api

# セキュリティ設定（開発用）
KANPO_API_KEY=api-key
ENVIRONMENT=development
DEBUG=true

# CORS設定（開発環境 - カンマ区切り）
ALLOWED_ORIGINS=https://sample.com
# ログレベル
LOG_LEVEL=DEBUG

# サービス制限
MAX_FILE_SIZE=10485760  # 10MB
TIMEOUT=30